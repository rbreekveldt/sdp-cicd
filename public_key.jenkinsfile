//Jenkins pipeline to whitelist IP's

pipeline {
    agent any
    stages {

        stage ("Update_Public_keys") {
            steps {
                ansiblePlaybook extras: "-e @access/public_keys -e @access/authorized_people", inventory: "inventory/public-key-inventory.yml", playbook: 'playbooks/add_public_keys.yml'
                }
            }
        }

    }
